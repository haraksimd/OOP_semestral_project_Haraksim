<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.messagesController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Date (YYYY-MM-DD)" />
      <TextField fx:id="messageDate" layoutX="14.0" layoutY="35.0" />
      <Label layoutX="14.0" layoutY="70.0" text="Message" />
      <TextArea fx:id="messageText" layoutX="14.0" layoutY="92.0" prefHeight="301.0" prefWidth="576.0" />
      <Button fx:id="sendMessage" layoutX="474.0" layoutY="35.0" mnemonicParsing="false" onAction="#sendMessage" text="Send" />
      <Button fx:id="messageBackBtn" layoutX="535.0" layoutY="35.0" mnemonicParsing="false" onAction="#messageBack" text="Back" />
   </children>
</AnchorPane>
