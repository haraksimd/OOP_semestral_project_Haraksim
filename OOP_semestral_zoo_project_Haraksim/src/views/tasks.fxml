<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="299.0" prefWidth="354.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.tasksController">
   <children>
      <Label layoutX="10.0" layoutY="14.0" prefHeight="22.0" prefWidth="93.0" style="-fx-font-weight: bold;" text="Task" />
      <TextField fx:id="dateTaskTextField" layoutX="14.0" layoutY="218.0" prefHeight="36.0" prefWidth="320.0" />
      <Label layoutX="14.0" layoutY="196.0" prefHeight="22.0" prefWidth="148.0" style="-fx-font-weight: bold;" text="Date (YYYY-MM-DD)" />
      <Button fx:id="submitTaskBtn" layoutX="267.0" layoutY="259.0" mnemonicParsing="false" onAction="#submitNewTask" text="Submit" />
      <Button fx:id="backTaskBtn" layoutX="14.0" layoutY="259.0" mnemonicParsing="false" onAction="#backTaskButton" text="Back" />
      <TextArea fx:id="taskTextArea" layoutX="14.0" layoutY="36.0" prefHeight="160.0" prefWidth="320.0" />
   </children>
</AnchorPane>
